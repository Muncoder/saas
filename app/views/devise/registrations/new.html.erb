<div class="row">
  <div class="col-md-6 col-md-offset-3 well">
      <% if params[:plan] == '2' %>
          <h1>Pro Account</h1>
          <p>Sign up for the pro account for <span class="label label-info">$10/month </span>  and get access to the community and contact information.</p>

          <%= render 'pro_form' %>          
      <% else %>
          <h1>Basic Account</h1>
          <p>Sign up for free and get basic access to the community.</p>          

          <%= render 'basic_form' %>
      <% end %>

      

      <%= render "devise/shared/links" %>
  </div>
</div>

